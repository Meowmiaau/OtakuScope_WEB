{
    "env": {
      "node": true,
      "commonjs": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12
      // "sourceType": "module"
    },
    "plugins": [
      "node"
    ],
    // "globals": {
    //   "require": "readonly",  // Defines globals
    //   "module": "readonly",
    //   "process": "readonly"
    // },
    "rules": {
      "no-undef": "error",
      "no-unused-vars": "warn",
      "node/no-unsupported-features/es-syntax": "off"
  }
